---
import Layout from '../layouts/Layout.astro';
import Navigation from '../components/Navigation.astro';
import TechFilters from '../components/TechFilters.astro';

const title="WORK";
const links = ['About', 'Contact'];

const { filters } = Astro.props

---
<Layout title={title}>
    <Navigation title={title} links={links}/>
    <TechFilters items={filters} transition:persist/>
    <slot/>
</Layout>
